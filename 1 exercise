<button>nazhmi na menya</button>
<div></div>
<script>
(function () {
    var mas = [???, !!!,];
    var n = 0;
    var but = document.getElementsByTagName("button")[0];
    var div = document.getElementsByTagName("div")[0];
    but.onclick = function () {
        div.innerHTML = mas[n];
        n++;
        if (!mas[n]) {
            n = 0;
        }
    }
    but.nazhmi na menya();
})();
</script>
